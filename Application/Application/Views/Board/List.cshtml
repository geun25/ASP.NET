@model List<Application.Models.Document>
@{
    Layout = null;
    var member = ViewData["Member"] as Application.Models.Member;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <div>
        <p>
            @member.Name 님 환영합니다.
        </p>
        <table>
            <thread>
                <tr>
                    <th>번호</th>
                    <th>제목</th>
                    <th>글쓴이</th>
                </tr>
            </thread>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Document_Number</td>
                        <td>@item.Title</td>
                        <td>@item.Writer</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
